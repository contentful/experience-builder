{
  "name": "@contentful/experiences-core",
  "version": "3.6.1-beta.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/experience-builder.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/contentful/experience-builder/issues"
  },
  "files": [
    "readme.md",
    "package.json",
    "dist/**/*.*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./constants": "./dist/constants.js",
    "./types": "./dist/types.d.ts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ],
      "constants": [
        "./dist/constants.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c ./rollup.config.mjs",
    "predev": "npm run clean",
    "dev": "rollup -c ./rollup.config.mjs --watch --environment DEV",
    "lint": "eslint src --ext '.ts,.tsx,.js,.jsx' --max-warnings 0 --ignore-path ../../.eslintignore",
    "lint:fix": "eslint src --ext '.ts,.tsx,.js,.jsx' --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "depcruise": "depcruise src"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.12",
    "@vitest/coverage-v8": "^2.1.1",
    "contentful": "^10.6.4",
    "happy-dom": "^13.3.8",
    "rimraf": "^5.0.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-ts": "^3.4.5",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@contentful/experiences-validators": "file:../validators",
    "@contentful/rich-text-types": "^17.0.0",
    "lodash-es": "^4.17.21",
    "zustand": "^4.4.7"
  },
  "peerDependencies": {
    "contentful": ">=10.6.0"
  }
}
