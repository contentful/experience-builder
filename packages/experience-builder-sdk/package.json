{
  "name": "@contentful/experiences-sdk-react",
  "version": "1.27.0-beta.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/src/index.d.ts",
  "type": "module",
  "author": "Contentful GmbH",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/experience-builder.git",
    "directory": "packages/experience-builder-sdk"
  },
  "bugs": {
    "url": "https://github.com/contentful/experience-builder/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "predev": "npm run clean",
    "dev": "rollup -c ./rollup.config.mjs --watch --environment DEV",
    "update-dist-version": "node ./bin/injectSDKVersion.cjs & node ./bin/searchAndReplaceSdkVersion.cjs",
    "prepare": "node ./bin/injectSDKVersion.cjs",
    "prebuild": "npm run clean && node ./bin/injectSDKVersion.cjs",
    "build": "rollup -c rollup.config.mjs",
    "postversion": "npm run update-dist-version",
    "watch": "vite build --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage --maxWorkers 2",
    "tsc": "tsc --noEmit",
    "lint": "eslint ./src --ext '.ts,.tsx,.js,.jsx'",
    "presemantic-release": "npm run build",
    "semantic-release": "semantic-release",
    "prettier:check": "prettier --check '**/*.{jsx,js,ts,tsx,css}'",
    "prettier": "prettier --write '**/*.{jsx,js,ts,tsx,css}'",
    "preview": "vite preview",
    "depcruise": "depcruise src"
  },
  "dependencies": {
    "@contentful/experiences-components-react": "file:../components",
    "@contentful/experiences-core": "file:../core",
    "@contentful/experiences-validators": "file:../validators",
    "@contentful/experiences-visual-editor-react": "file:../visual-editor",
    "@contentful/rich-text-types": "^16.2.1",
    "classnames": "^2.3.2",
    "csstype": "^3.1.2",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "style-inject": "^0.3.0",
    "vite-plugin-css-injected-by-js": "^3.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@semantic-release/changelog": "^6.0.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.6",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.11.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "contentful": "^10.6.14",
    "glob": "^10.4.1",
    "husky": "^9.0.10",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^24.0.0",
    "postcss-import": "^16.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "relative-deps": "^1.0.7",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^7.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "semantic-release": "23.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2",
    "vite-plugin-svgr": "^4.1.0"
  },
  "peerDependencies": {
    "contentful": ">=10.6.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "files": [
    "dist",
    "package.json"
  ]
}
