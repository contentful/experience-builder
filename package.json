{
  "name": "@contentful/experience-builder",
  "version": "0.0.1-alpha-1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "author": "Contentful GmbH",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "VITE_SDK_VERSION=$npm_package_version vite build",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "check-types": "tsc",
    "lint": "eslint src --ext '.ts,.tsx,.js,.jsx'",
    "presemantic-release": "yarn build",
    "semantic-release": "semantic-release",
    "prettier:check": "prettier --check '**/*.{jsx,js,ts,tsx,css}'",
    "prettier": "prettier --write '**/*.{jsx,js,ts,tsx,css}'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@contentful/rich-text-types": "^16.2.1",
    "@contentful/visual-sdk": "^1.0.0-alpha.9",
    "classnames": "^2.3.2",
    "contentful": "^10.3.0",
    "csstype": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "semantic-release": "19.0.5",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.0.1"
  },
  "packageManager": "yarn@3.5.0",
  "files": [
    "dist",
    "package.json"
  ],
  "release": {
    "branches": [
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "successCmd": "./bin/release-ctfl-eap.sh"
        }
      ]
    ]
  }
}
